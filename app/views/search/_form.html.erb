		  	<%= form_for @movie, html: { role: "form" } do |f| %>
		  	<div class="form-group">
			  	<%= f.hidden_field :title, value: movie["title"], class: "form-control" %>
			  	</div>
			  	<div class="form-group">
			  	<%= f.hidden_field :release_date, value: movie["release_date"], class: "form-control"  %>
			  	</div>
			  	<div class="form-group">
			  	<%= f.hidden_field :tmdb_id, value: movie["id"], class: "form-control"  %>
			  	</div>
			  	<div class="form-group">
			  	<%= f.hidden_field :poster, value: movie["poster_path"], class: "form-control"  %>
			  	</div>
			  	<div class="form-group">
			  	<%= f.hidden_field :backdrop, value: movie["backdrop_path"], class: "form-control"  %>
			  	</div>
			  	
			  	<div class="form-group">
			  	<%= f.label :download_link %>
			  	<%= f.text_field :download_link, class: "form-control" %>
			  	</div>
			  	<div class="form-group">
			  	<%= f.label :magnetic_link %>
			  	<%= f.text_field :magnetic_link, class: "form-control" %>
			  	</div>
			  	<div class="form-group">
			  	<%= f.label :torrent_file_link %>
			  	<%= f.text_field :torrent_file_link, class: "form-control" %>
			  	</div>
			   	<%= f.submit "Save Movie", class: "btn btn-sm btn-color" %>
			  <% end %>